/*Lee dos valores de potenciometros y los escribe por el serial en formato X,Y/n*/int buttonPin = 5;void setup() {  Serial.begin(9600);  pinMode(buttonPin, INPUT);}void loop() {  int x = analogRead(A0); //Pot x  int y = analogRead(A1); //Pot y  int temp = analogRead(A2); //LM35  int lum = analogRead(A3); //Fotoresistencia  int hue = analogRead(A4); //Pot color  int b = digitalRead(buttonPin);    Serial.print(x);  Serial.print(",");  Serial.print(y);  Serial.print(",");  Serial.print(hue);  Serial.print(",");  Serial.print(temp);  Serial.print(",");  Serial.print(lum);  Serial.print(",");  Serial.println(b);    delay(16);}